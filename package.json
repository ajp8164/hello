{
  "name": "hello",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:prod": "react-native run-android --mode release",
    "sign:android": "npm run env:prod; rm -f android/app/build/outputs/bundle/release/app-release.aab; npx react-native build-android --mode=release",
    "open:android": "open -a open -a /Applications/Android\\ Studio.app android",
    "ios": "react-native run-ios --scheme 'Hello debug' --simulator 'iPhone 16'",
    "ios:large": "react-native run-ios --scheme 'Hello debug' --simulator 'iPhone 16 Plus'",
    "ios:prod": "react-native run-ios --scheme 'Hello' --simulator 'iPhone 16' --configuration Release",
    "ios:url": "xcrun simctl openurl booted",
    "lint": "eslint .",
    "postinstall": "patch-package; npm run prepare:ios;",
    "prepare:ios": "cd ios; (pod && RCT_NEW_ARCH_ENABLED=0 pod install) || echo 'skipping pod install, command not found'",
    "metro:clean": "rm -fr $TMPDIR/metro-cache",
    "clean:everything": "rm -rf node_modules; rm -rf package-lock.json; cd ios; rm -rf build; rm -rf Pods; rm -rf Podfile.lock; cd ..; npm run metro:clean; echo 'Recommended: Reset cache after next install `npm run start --reset-cache`'; echo 'Recommended: Delete Xcode derived data'; echo ",
    "open:ios": "open ios/*.xcworkspace",
    "start": "react-native start",
    "env:dev": "rm .env; ln -s .env.dev .env;",
    "env:mock": "rm .env; ln -s .env.mock .env;",
    "env:prod": "rm .env; ln -s .env.production .env;",
    "test": "jest"
  },
